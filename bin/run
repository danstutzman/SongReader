#!/bin/sh

MAIN=Ocr4Music
#MAIN=WidthSpeculator
#MAIN=FullPage
DEFAULT_ARGS=

cd `dirname $0`/..
bin/build
EXIT=$?
if [ "$EXIT" == "0" ]; then
  echo Running...
  java -Xmx1024M -Djava.awt.headless=true -cp build/classes:/Applications/scala-2.8.1/lib/scala-library.jar $MAIN $DEFAULT_ARGS "$@"
  EXIT=$?
  echo "bin/run: Got exit code of $EXIT"
  exit $EXIT
else
  exit $EXIT
fi
